<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="607.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.EmailContentController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="33.0" prefWidth="500.0" style="-fx-background-color: #8c8c8c;">
         <children>
            <Button fx:id="goBackButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="69.0" text="GoBack" />
            <Label prefHeight="17.0" prefWidth="169.0" />
            <Label fx:id="title" prefHeight="25.0" prefWidth="126.0" text="Title: ">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <TextArea fx:id="mailContent" disable="true" prefHeight="344.0" prefWidth="500.0" />
   </children>
</VBox>
